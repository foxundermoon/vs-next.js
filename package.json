{
  "name": "next-js",
  "displayName": "next.js",
  "description": "next.js enhance",
  "version": "0.0.2",
  "publisher": "foxundermoon",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": ["Other"],
  "icon": "https://raw.githubusercontent.com/foxundermoon/vs-next.js/master/image/next-white.png",
  "activationEvents": ["onCommand:extension.sayHello"],
  "main": "./out/src/extension",
  "contributes": {
    "grammars": [
      {
        "injectTo": ["source.js", "source.ts", "source.jsx", "source.tsx"],
        "scopeName": "inline.css",
        "path": "./syntaxes/next-js1.json",
        "embeddedLanguages": {
          "meta.embedded.block.css": "css"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "galleryBanner": {
    "color": "#2e2348",
    "theme": "dark"
},
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
